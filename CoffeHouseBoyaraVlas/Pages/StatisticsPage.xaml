<Page x:Class="CoffeHouseBoyaraVlas.Pages.StatisticsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CoffeHouseBoyaraVlas.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StatisticsPage">

    <Grid>
        <StackPanel>
            <Border BorderBrush="#FF634919"
                            BorderThickness="3">


                <Frame x:Name="StatsMainFrame"/>

            </Border>
            <WrapPanel Height="50" Background="#FF43BF43">
                <Button x:Name="btnSaleStats" Content="Продажи"
                    Margin="10 5" Height="40"
                    Background="#FF634919"
                    Foreground="Wheat" Click="btn_addProd_Click"/>
            </WrapPanel>

        </StackPanel>
    </Grid>
</Page>
